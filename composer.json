{
    "name": "j0hnys/trident",
    "description": "a laravel code generator for developing applications following Domain Driven Design (DDD) principles",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "John Stamkos",
            "email": "tzonyss@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "mustache/mustache": "2.12.0",
        "doctrine/dbal": "2.9.1",
        "symfony/class-loader": "^2.3|3.*",
        "krlove/eloquent-model-generator": "1.3.2",
        "xethron/migrations-generator": "2.0.2",
        "nikic/php-parser": "4.2.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "j0hnys\\Trident\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "j0hnys\\Trident\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "j0hnys\\Trident\\TridentServiceProvider"
            ]
        }
    }
}
